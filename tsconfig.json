{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "db/models/User.js", "db/models/Post.ts", "db/conn.ts", "db/passport.js", "pages/api/auth/[...nextauth].ts", "pages/api/auth/[...nextauth].ts", "db/utils/Post.ts"],
  "exclude": ["node_modules"]
}
